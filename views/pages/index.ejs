<%- include('../partials/header', { title: "Message Board" }) %>
<main class="container py-5">
    <h1>All Messages</h1>
    <ul>
        <% for (let message of messages) { %>
        <li>
            <div><%= message.user %></div>
            <p><%= message.text %></p>
            <div><%= message.added %></div>
        </li>
        <% } %>
    </ul>
</main>
<%- include('../partials/footer') %>
